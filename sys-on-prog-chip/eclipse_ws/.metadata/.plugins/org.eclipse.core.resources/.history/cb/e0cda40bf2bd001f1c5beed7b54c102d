#include "io.h"
#include <stdio.h>
#include "system.h"

void set_led(unsigned char led_pattern){
	IOWR(LED_BASE,0, led_pattern);
}

int main(){
	unsigned char led_pattern = 0x00;
	printf("PROJET_Co_Design \n");
	while(1){
		led_pattern = IORD(SWITCHER_BASE, 0);
		printf("%X \n", SWITCHER_BASE);
		IOWR(LED_BASE, 0, 0x55);

		//set_led(led_pattern);
	}
}
